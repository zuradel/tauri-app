{
  "name": "tauri-app",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev-reqs": "pip install -r requirements.txt",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "build:fastapi": "python3 -m PyInstaller -c -F --clean --name main --distpath src-tauri/bin/api backends/main.py",
    "build:fastapi-macos": "python3 -m PyInstaller -c -F --clean --name main-aarch64-apple-darwin --distpath src-tauri/bin/api backends/main.py",
    "tauri": "tauri"
  },
  "dependencies": {
    "@radix-ui/react-dialog": "^1.1.13",
    "@radix-ui/react-dropdown-menu": "^2.1.14",
    "@radix-ui/react-select": "^2.2.4",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-tabs": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.6",
    "@tailwindcss/vite": "^4.1.6",
    "@tauri-apps/api": "^2.5.0",
    "@tauri-apps/plugin-opener": "^2",
    "@tauri-apps/plugin-shell": "~2",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "jotai": "^2.12.4",
    "lucide-react": "^0.509.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-resizable-panels": "^3.0.1",
